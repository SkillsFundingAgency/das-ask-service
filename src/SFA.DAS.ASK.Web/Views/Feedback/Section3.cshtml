@model SFA.DAS.ASK.Web.Controllers.Feedback.ViewModels.Section3ViewModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<a asp-action="Index" asp-controller="FeedbackSection2" asp-route-feedbackId="@Model.FeedbackId" class="govuk-back-link">Back</a>

<main class="govuk-main-wrapper govuk-main-wrapper--auto-spacing" id="main-content" role="main">
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">
            <h1 class="govuk-heading-xl">
                How well did your ASK visit achieve each objective?
                <span class="govuk-caption-xl">Section 3 of 7</span>
            </h1>
        </div>
    </div>

    <partial name="_ErrorSummary"/>
    
    <div class="govuk-grid-row">
            <div class="govuk-grid-column-full">
                <form method="post">
                    <partial name="_FeedbackRadios" for="DemonstratingDifferentTypesOfEmployers"/>
                    <partial name="_FeedbackRadios" for="IncreasingAwarenessOfHigherAndDegree"/>
                    <partial name="_FeedbackRadios" for="MakingStaffMoreConfident"/>
    
                    <button class="govuk-button" data-module="govuk-button">Continue</button>
    
                    <a href="/feedback-form/index" class="das-button--link">Cancel</a>
                </form>
                <p class="govuk-hint">If you experience any difficulties in completing the form, please call for assistance on <a href="tel:01462 341191">01462 341191</a>.</p>
            </div>
        </div>
</main>